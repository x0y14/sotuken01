   "source": [
    "import numpy as np\n",
    "from sklearn.decomposition import PCA\n",
    "\n",
    "# numpyの行列を作成\n",
    "matrix = np.array([[1, 2, 3], [3, 4, 5], [5, 6, 7]])\n",
    "\n",
    "# PCAのインスタンスを作成\n",
    "pca = PCA(n_components=1)\n",
    "\n",
    "# PCAを適用\n",
    "matrix_pca = pca.fit_transform(matrix)\n",
    "\n",
    "print(matrix_pca)"
   ],
